class A{
    protected int x;
    protected int y;

    int getx(){return this.x;}
    int gety(){return this.y;}

    void setx(int x){
        this.x=x;}


    void sety(int a){this.y=a;}

    int ajoute(int a, int b){
        int res = this.x+a+this.y+b;
        return res;
    }
}



